{
  "name": "@netlify/integrations",
  "version": "0.2.1",
  "description": "",
  "type": "module",
  "main": "./dist/main.js",
  "exports": "./dist/main.js",
  "types": "./dist/main.d.ts",
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "test": "vitest",
    "test:ci": "vitest run"
  },
  "config": {
    "eslint": "--ignore-path .gitignore --cache --format=codeframe --max-warnings=0 \"{src,scripts,.github}/**/*.{js,ts,md,html}\" \"*.{js,ts,md,html}\" \".*.{js,ts,md,html}\"",
    "prettier": "--ignore-path .gitignore --loglevel=warn \"{src,scripts,.github}/**/*.{js,ts,md,yml,json,html}\" \"*.{js,ts,yml,json,html}\" \".*.{js,ts,yml,json,html}\" \"!**/package-lock.json\" \"!package-lock.json\""
  },
  "ava": {
    "verbose": true
  },
  "keywords": [],
  "license": "MIT",
  "repository": "netlify/integrations",
  "bugs": {
    "url": "https://github.com/netlify/integrations/issues"
  },
  "author": "Netlify Inc.",
  "directories": {
    "test": "test"
  },
  "devDependencies": {
    "@netlify/eslint-config-node": "^7.0.1",
    "ava": "^4.0.0",
    "c8": "^7.11.0",
    "typescript": "^4.5.4",
    "vitest": "^0.25.2"
  },
  "engines": {
    "node": "^14.16.0 || >=16.0.0"
  },
  "dependencies": {
    "@netlify/functions": "^1.3.0"
  }
}
